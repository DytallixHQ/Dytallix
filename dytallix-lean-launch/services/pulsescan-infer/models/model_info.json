{
  "version": "1.0.0",
  "type": "ensemble",
  "created_at": "2024-01-01T00:00:00Z",
  "description": "PulseScan anomaly detection ensemble model",
  "components": [
    {
      "name": "isolation_forest",
      "type": "unsupervised",
      "weight": 0.3,
      "parameters": {
        "n_estimators": 100,
        "contamination": 0.1,
        "random_state": 42
      }
    },
    {
      "name": "one_class_svm", 
      "type": "unsupervised",
      "weight": 0.25,
      "parameters": {
        "nu": 0.1,
        "gamma": "scale",
        "kernel": "rbf"
      }
    },
    {
      "name": "autoencoder",
      "type": "neural_network",
      "weight": 0.25,
      "parameters": {
        "hidden_layers": [64, 32, 16, 32, 64],
        "activation": "relu",
        "epochs": 100,
        "batch_size": 32
      }
    },
    {
      "name": "statistical_tests",
      "type": "statistical",
      "weight": 0.2,
      "parameters": {
        "z_score_threshold": 2.5,
        "percentile_threshold": 95
      }
    }
  ],
  "features": {
    "count": 20,
    "names": [
      "velocity_1h", "velocity_24h", "velocity_7d",
      "amount_z_score", "amount_percentile", "round_amount_indicator",
      "hour_of_day", "day_of_week", "is_weekend", "time_since_last_tx",
      "in_degree", "out_degree", "clustering_coefficient", 
      "betweenness_centrality", "page_rank",
      "gas_price_z_score", "gas_limit_z_score", 
      "tx_frequency_pattern", "address_age_days", "unique_counterparties"
    ],
    "normalization": "z_score",
    "scaling_params": {
      "mean": [0.5, 0.3, 0.1, 0.0, 0.5, 0.1, 0.5, 0.5, 0.3, 0.4, 0.2, 0.3, 0.1, 0.05, 0.15, 0.0, 0.0, 0.6, 100.0, 8.0],
      "std": [0.3, 0.2, 0.05, 1.0, 0.3, 0.3, 0.3, 0.3, 0.5, 0.4, 0.15, 0.2, 0.08, 0.03, 0.1, 1.0, 0.8, 0.3, 50.0, 5.0]
    }
  },
  "performance": {
    "accuracy": 0.875,
    "precision": 0.823,
    "recall": 0.789,
    "f1_score": 0.806,
    "auc_roc": 0.912,
    "evaluation_date": "2024-01-01",
    "test_set_size": 10000,
    "validation_method": "stratified_k_fold",
    "k_folds": 5
  },
  "thresholds": {
    "low": 0.3,
    "medium": 0.5,
    "high": 0.7,
    "critical": 0.9
  },
  "training": {
    "dataset_size": 50000,
    "training_duration_hours": 8.5,
    "hyperparameter_optimization": true,
    "cross_validation": true,
    "feature_importance": {
      "velocity_24h": 0.15,
      "amount_z_score": 0.12,
      "clustering_coefficient": 0.11,
      "page_rank": 0.10,
      "velocity_1h": 0.09,
      "gas_price_z_score": 0.08,
      "betweenness_centrality": 0.07,
      "tx_frequency_pattern": 0.06,
      "time_since_last_tx": 0.05,
      "out_degree": 0.05,
      "in_degree": 0.04,
      "velocity_7d": 0.03,
      "round_amount_indicator": 0.02,
      "is_weekend": 0.01,
      "hour_of_day": 0.01,
      "day_of_week": 0.01
    }
  }
}