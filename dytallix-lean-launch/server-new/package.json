{
  "name": "@dytallix/server",
  "version": "1.0.0",
  "description": "Dytallix production-lean backend/API service",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "migrate": "tsx src/migrations/runner.ts",
    "seed": "tsx src/migrations/seed.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "backup": "tsx src/util/backup.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "better-sqlite3": "^9.3.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.2",
    "pino": "^8.17.2",
    "prom-client": "^15.1.0",
    "undici": "^6.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.1",
    "supertest": "^6.3.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
