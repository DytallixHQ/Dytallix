# =====================
# Frontend (using VITE_* naming for build-time availability)
# =====================
REACT_APP_API_BASE_URL=https://api.testnet.dytallix.com
REACT_APP_DEV_MODE=true
REACT_APP_NODE_URL=http://localhost:3003
REACT_APP_EXPLORER_URL=http://localhost:3032

# =====================
# Runtime Environment - UNIFIED PORT SCHEME
# =====================
NODE_ENV=development
PORT=3001
ALLOWED_ORIGIN=http://localhost:3000

# Development Ports (SINGLE SOURCE OF TRUTH)
FRONTEND_PORT=3000
BACKEND_API_PORT=3001
QUANTUMVAULT_API_PORT=3002
BLOCKCHAIN_NODE_PORT=3003
WEBSOCKET_PORT=3004
FAUCET_PORT=3005

# =====================
# Cosmos Chain Configuration
# =====================
VITE_LCD_HTTP_URL=http://localhost:1317
VITE_RPC_HTTP_URL=http://localhost:26657
VITE_RPC_WS_URL=ws://localhost:26657/websocket
VITE_CHAIN_ID=dytallix-local
CHAIN_PREFIX=dytallix
RPC_HTTP_URL=http://localhost:26657

# =====================
# API and Faucet Configuration (updated ports)
# =====================
VITE_API_URL=http://localhost:3001
VITE_QUANTUMVAULT_API_URL=http://localhost:3002
VITE_BLOCKCHAIN_URL=http://localhost:3003
VITE_WEBSOCKET_URL=ws://localhost:3004

# Backend server configuration for blockchain node connection
NODE_RPC_URL=http://localhost:3003
BLOCKCHAIN_NODE_URL=http://localhost:3003

# =====================
# Faucet Signer (DO NOT USE production mnemonics in dev/test)
# =====================
# FAUCET_MNEMONIC="your test mnemonic here - NEVER commit real mnemonics"
FAUCET_GAS_PRICE=0.025uDRT
FAUCET_MAX_PER_REQUEST_DGT=2
FAUCET_MAX_PER_REQUEST_DRT=50
FAUCET_COOLDOWN_MINUTES=60

# =====================
# Rate Limiting with Redis (Optional)
# =====================
# DLX_RATE_LIMIT_REDIS_URL=redis://localhost:6379
# Leave unset to use in-memory rate limiting

# =====================
# Security Feature Flags
# =====================
ENABLE_SEC_HEADERS=1
ENABLE_CSP=1

# =====================
# Node Block Producer (Rust devnet)
# =====================
DYT_BLOCK_INTERVAL_MS=15000
DYT_EMPTY_BLOCKS=true
BLOCK_MAX_TX=100
DYT_WS_ENABLED=true
DYT_DATA_DIR=./data
DYT_CHAIN_ID=dytallix-testnet-1
RUNTIME_MOCKS=false

# =====================
# Governance Parameters
# (CLI fallback if flags not provided; maps to dcli --gov.* flags)
# =====================
GOV_QUORUM=0.33
GOV_THRESHOLD=0.50
GOV_VETO=0.334
GOV_PROPOSAL_DEPOSIT=1000000
GOV_MIN_VOTE_DURATION=3600
GOV_MAX_VOTE_DURATION=86400
GOV_VETO_THRESHOLD=0.334

