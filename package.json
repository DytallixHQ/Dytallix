{
  "dependencies": {
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "node-fetch": "^2.7.0",
    "ws": "^8.18.3"
  },
  "scripts": {
    "start:faucet:dev": "npm --prefix dytallix-lean-launch run dev:full",
    "build:faucet": "npm --prefix dytallix-lean-launch run build",
    "self-test": "bash scripts/test-pqc.sh",
    "gen:pqclean-manifest": "node scripts/gen-pqclean-manifest.js",
    "test": "jest --runInBand",
    "proxy:dev": "node api-proxy.js",
    "frontend:dev": "npm --prefix dytallix-lean-launch run dev",
    "dev:proxy": "npm run proxy:dev",
    "dev:ui": "npm run frontend:dev",
    "dev:all": "concurrently -k -r -s first \"npm:proxy:dev\" \"npm:frontend:dev\"",
    "test:ui": "npm --prefix dytallix-lean-launch run test",
    "ws:capture": "node dytallix-lean-launch/scripts/ws_capture.js"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "jest": "^29.7.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "testEnvironment": "node"
  }
}
