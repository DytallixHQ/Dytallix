# Development Environment Configuration
VITE_ENVIRONMENT=development
VITE_APP_NAME=Dytallix

# Local Development API Endpoints
VITE_BLOCKCHAIN_API_URL=http://localhost:3030
VITE_AI_API_URL=http://localhost:8000
VITE_WEBSOCKET_URL=ws://localhost:3030/ws

# Development Settings
VITE_LOG_LEVEL=debug
VITE_ENABLE_DEBUG_TOOLS=true
VITE_ENABLE_MOCK_DATA=true

# Blockchain Configuration
VITE_NETWORK_NAME=development
VITE_CHAIN_ID=1337
VITE_CONFIRMATION_BLOCKS=1

# AI Services Configuration
VITE_AI_ANALYSIS_ENABLED=true
VITE_AI_TIMEOUT=15000

# Wallet Configuration
VITE_ENABLE_METAMASK=true
VITE_ENABLE_PQC_WALLET=true